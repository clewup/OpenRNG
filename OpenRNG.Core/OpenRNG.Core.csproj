<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <PackageId>OpenRNG.Core</PackageId>
        <Version>1.0.0</Version>
        <Authors>Lewis Johnson</Authors>
        <Company>Lewis Johnson</Company>
        <Description>Reusable cryptographic randomness and entropy services</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/clewup/OpenRNG</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.5" />
    </ItemGroup>

</Project>
